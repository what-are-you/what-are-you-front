{
    "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
    "rules": {
        "no-var": "warn", // var 금지
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto",
                "useTabs": false
            }
        ] //delete 'cr' prettier/prettier 오류를 피하기위해 윈도우 유저에게 필요한 부분
    }
}
